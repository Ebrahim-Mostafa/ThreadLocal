<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="InstabugTask" thread-count="3" parallel="tests">
    <test name="SearchTestOnChrome">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="SearchTest.GoogleRun"/>
        </classes>
    </test>
    <test name="SearchTestOnFireFox" >
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="SearchTest.GoogleRun"/>
        </classes>
    </test>
    <test name="SearchTestOnSafari" >
        <parameter name="browser" value="Safari"/>
        <classes>
            <class name="SearchTest.GoogleRun"/>
        </classes>
    </test>
</suite>